<laserDevice>
  <originBottomLeft>false</originBottomLeft>
  <jobSentText>Job was sent as &apos;$jobname&apos;
Please:
-Close the lid
-Turn on the Ventilation
-And press &apos;start&apos; on the Lasercutter $name</jobSentText>
  <jobPrefix>visicut </jobPrefix>
  <laserCutter class="com.t_oster.liblasercut.drivers.ThunderLaser">
    <ruida>
      <filename>thunder.rd</filename>
      <port>80</port>
      <hostname></hostname>
      <boundingWidth>0.0</boundingWidth>
      <boundingHeight>0.0</boundingHeight>
      <boundingLayer>-1</boundingLayer>
    </ruida>
    <useBidirectionalRastering>false</useBidirectionalRastering>
    <addSpacePerRasterLine>2.0</addSpacePerRasterLine>
    <BedWidth>900.0</BedWidth>
    <BedHeight>600.0</BedHeight>
    <LaserPowerMin>0</LaserPowerMin>
    <LaserPowerMax>70</LaserPowerMax>
    <MaxVectorCutSpeed>1000</MaxVectorCutSpeed>
    <MaxVectorMoveSpeed>1000</MaxVectorMoveSpeed>
    <filename>/dev/ttyUSB0</filename>
  </laserCutter>
  <cameraTiming>0</cameraTiming>
  <projectorTiming>0</projectorTiming>
  <projectorWidth>0</projectorWidth>
  <projectorHeight>0</projectorHeight>
  <description>Thunderlaser am USB Port</description>
  <thumbnailPath>ThunderLaser.png</thumbnailPath>
  <name>ThunderUSB</name>
</laserDevice>
